<? 
    // Funcin para descargar el archivo de exportacin de logs en formato TXT
    function download()
    {
        $name = $_SESSION["logName"];
        unset($_SESSION["logName"]);
        header("Content-type: application/txt"); 
				header("Content-Disposition: attachment; filename=".$name); 
				readfile("../tmp/".$name);
		}
		
		download();
?>